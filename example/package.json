{
  "name": "with-redux-observable",
  "version": "2.4.1",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@christophediprima/razzle-react-redux-observable-found": "link:..",
    "@material-ui/core": "^3.9.1",
    "express": "^4.16.2",
    "farce": "^0.2.6",
    "found": "0.3",
    "found-scroll": "^0.1.5",
    "isomorphic-fetch": "^2.2.1",
    "jss": "^9.8.7",
    "razzle": "^2.4.1",
    "razzle-plugin-typescript": "^2.4.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-jss": "^8.6.1",
    "react-redux": "5",
    "react-redux-epic": "^1.1.0",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^23.3.13",
    "@types/jss": "^9.5.7",
    "@types/node": "^10.12.19",
    "@types/react": "^16.7.22",
    "@types/react-dom": "^16.0.11",
    "@types/react-jss": "^8.6.1",
    "@types/react-redux": "5",
    "@types/react-redux-epic": "^1.1.2",
    "@types/webpack-env": "^1.13.6",
    "prettier-tslint": "^0.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
